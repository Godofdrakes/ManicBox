<viewBase:WindowView
	x:Class="ManicBox.Preview.MainWindow"
	x:TypeArguments="viewModel:WindowViewModel"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:local="clr-namespace:ManicBox.Preview"
	xmlns:viewModel="clr-namespace:ManicBox.Reactive.ViewModelBase;assembly=ManicBox.Reactive"
	xmlns:viewBase="clr-namespace:ManicBox.Reactive.ViewBase;assembly=ManicBox.Reactive"
	xmlns:view="clr-namespace:ManicBox.Preview.View"
	mc:Ignorable="d"
	Title="MainWindow" Height="450" Width="800">
	<DockPanel>
		<TextBlock x:Name="TextBlock"
			DockPanel.Dock="Top"
			FontSize="16"
			Text="Foreground Window"
			TextAlignment="Center" />

		<view:ThumbnailView x:Name="ThumbnailView" />
	</DockPanel>
</viewBase:WindowView>