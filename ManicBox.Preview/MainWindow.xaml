<viewBase:WindowView
	x:Class="ManicBox.Preview.MainWindow"
	x:TypeArguments="viewModel:WindowViewModel"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:local="clr-namespace:ManicBox.Preview"
	xmlns:viewModel="clr-namespace:ManicBox.Reactive.ViewModelBase;assembly=ManicBox.Reactive"
	xmlns:viewBase="clr-namespace:ManicBox.Reactive.ViewBase;assembly=ManicBox.Reactive"
	xmlns:view="clr-namespace:ManicBox.Preview.View"
	mc:Ignorable="d"
	Title="MainWindow" Height="450" Width="800">
	<Grid>
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="1*" />
			<ColumnDefinition Width="2*" />
		</Grid.ColumnDefinitions>

		<view:WindowListView x:Name="WindowListView"
			Grid.Column="0" />

		<view:ThumbnailView x:Name="ThumbnailView"
			Grid.Column="1" />
	</Grid>
</viewBase:WindowView>